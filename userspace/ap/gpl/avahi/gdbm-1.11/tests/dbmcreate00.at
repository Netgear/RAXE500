AT_SETUP([create database])
AT_KEYWORDS([dbm create create00 dbmcreate00])

AT_CHECK([
AT_COMPAT_PREREQ
AT_SORT_PREREQ
num2word 1:1000 > input
dtload test < input
dtdump test | sort -k1,2 -n > output
cmp -s input output || diff -u input output
])

AT_CLEANUP

