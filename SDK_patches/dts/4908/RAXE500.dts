		compatible = "brcm,parallel-led";
		hardware_led;
		active_low;
		link = <(LED_SPEED_10|LED_SPEED_100)>;
		activity = <(LED_SPEED_10|LED_SPEED_100)>;
		status = "okay";
	};
	/* GPHY3_1G */
	led17: port_3-led-0-pin_6 {
		compatible = "brcm,parallel-led";
		hardware_led;
		active_high;
		link = <LED_SPEED_1G>;
        activity = <LED_SPEED_1G>;
		status = "okay";
	};
	/* GPHY3_100/10M */
	led18: port_3-led-1-pin_7 {
		compatible = "brcm,parallel-led";
