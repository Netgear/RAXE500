		active_high;
		link = <(LED_SPEED_10|LED_SPEED_100)>;
		activity = <(LED_SPEED_10|LED_SPEED_100)>;
		status = "okay";
	};

	/* Wi-Fi_5G-1 */
    led20:led_gpio_8 {
		compatible = "brcm,gpio-led";
		software_led;
		pin = <8>;
        active_high;
		init_low;
	};
	/* Wi-Fi_5G-2 */
	led21:led_gpio_9 {
		compatible = "brcm,gpio-led";
		software_led;
		pin = <9>;
		active_high;
        init_low;
	};
	/* WPS */
	led10:led_gpio_13 {
		compatible = "brcm,gpio-led";
		software_led;
		pin = <13>;
		active_high;
        init_low;
	};
	/* 2.5GE */
	led22:led_gpio_20 {
		compatible = "brcm,gpio-led";
